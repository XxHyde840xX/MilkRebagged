<?xml version="1.0" encoding="utf-8" ?>
<Patch>
    <Operation Class="PatchOperationFindMod">
        <mods>
            <li>Alpha Animals</li>
        </mods>
        <match Class="PatchOperationSequence">
            <success>Always</success>
            <operations>
                <li Class="PatchOperationReplace">
                    <success>Always</success>
                    <xpath>/Defs/ThingDef[defName = "AA_BlueMilk"]/graphicData</xpath>
                    <value>
                        <graphicData>
                            <texPath>Things/Items/Resources/AA_BlueMilk</texPath>
                            <graphicClass>Graphic_StackCount</graphicClass>
                        </graphicData>
                    </value>
                </li>
                <li Class="PatchOperationReplace">
                    <success>Always</success>
                    <xpath>/Defs/ThingDef[defName = "AA_GowMilk"]/graphicData</xpath>
                    <value>
                        <graphicData>
                            <texPath>Things/Items/Resources/AA_GowMilk</texPath>
                            <graphicClass>Graphic_StackCount</graphicClass>
                        </graphicData>
                    </value>
                </li>
                <li Class="PatchOperationReplace">
                    <success>Always</success>
                    <xpath>/Defs/ThingDef[defName = "AA_NightMilk"]/graphicData</xpath>
                    <value>
                        <graphicData>
                            <texPath>Things/Items/Resources/AA_NightMilk</texPath>
                            <graphicClass>Graphic_StackCount</graphicClass>
                        </graphicData>
                    </value>
                </li>
            </operations>
        </match>
    </Operation>
</Patch>